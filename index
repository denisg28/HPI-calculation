<p class="p1"><span class="s1">&lt;body&gt;</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">  </span>&lt;div class="container"&gt;</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">    </span>&lt;h2&gt;Date Difference Calculator&lt;/h2&gt;</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">    </span>&lt;form id="dateForm"&gt;</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>&lt;label for="date1"&gt;Enter Date 1:&lt;/label&gt;</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>&lt;input type="text" id="date1" placeholder="YYYY-MM-DDTHH:mm:ss.SSSZ" required&gt;</span></p>
<p class="p2"><span class="s1"></span><br></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>&lt;label for="date2"&gt;Enter Date 2:&lt;/label&gt;</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>&lt;input type="text" id="date2" placeholder="YYYY-MM-DDTHH:mm:ss.SSSZ" required&gt;</span></p>
<p class="p2"><span class="s1"></span><br></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>&lt;button type="button" onclick="calculateDifference()"&gt;Submit&lt;/button&gt;</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">    </span>&lt;/form&gt;</span></p>
<p class="p2"><span class="s1"></span><br></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">    </span>&lt;div id="result"&gt;&lt;/div&gt;</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">  </span>&lt;/div&gt;</span></p>
<p class="p2"><span class="s1"></span><br></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">  </span>&lt;script&gt;</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">    </span>function calculateDifference() {</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>// Get input values</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>const date1Str = document.getElementById('date1').value.trim();</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>const date2Str = document.getElementById('date2').value.trim();</span></p>
<p class="p2"><span class="s1"></span><br></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>// Convert input strings to Date objects</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>const date1 = new Date(date1Str);</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>const date2 = new Date(date2Str);</span></p>
<p class="p2"><span class="s1"></span><br></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>// Check if input is valid</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>if (isNaN(date1.getTime()) || isNaN(date2.getTime())) {</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">        </span>alert('Invalid date format. Please use the format: YYYY-MM-DDTHH:mm:ss.SSSZ');</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">        </span>return;</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>}</span></p>
<p class="p2"><span class="s1"></span><br></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>// Calculate time difference in milliseconds</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>const timeDifferenceMs = Math.abs(date1 - date2);</span></p>
<p class="p2"><span class="s1"></span><br></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>// Convert milliseconds to hours, minutes, and seconds</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>const hours = Math.floor(timeDifferenceMs / (1000 * 60 * 60));</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>const minutes = Math.floor((timeDifferenceMs % (1000 * 60 * 60)) / (1000 * 60));</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>const seconds = Math.floor((timeDifferenceMs % (1000 * 60)) / 1000);</span></p>
<p class="p2"><span class="s1"></span><br></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>// Display the result</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>const resultElement = document.getElementById('result');</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">      </span>resultElement.textContent = `HPI: ${hours}:${String(minutes).padStart(2, '0')}:${String(seconds).padStart(2, '0')}`;</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">    </span>}</span></p>
<p class="p1"><span class="s1"><span class="Apple-converted-space">  </span>&lt;/script&gt;</span></p>
<p class="p1"><span class="s1">&lt;/body&gt;</span></p>
<p class="p1"><span class="s1">&lt;/html&gt;</span></p>
